API_TOKEN=changeme
PORT=4000
# Cron: second minute hour day-of-month month day-of-week (node-cron format)
CRON_SPEC=0 2 * * *
# Set to true to disable cron scheduling (manual ingest only)
DISABLE_CRON=false
# Apify fallback (optional, required if Google Trends blocks your network)
APIFY_TOKEN=
# Task or actor to run; defaults to petrpatek~google-trends-scraper if unset
APIFY_TASK=
